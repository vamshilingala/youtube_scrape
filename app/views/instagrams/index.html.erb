<p id="notice"><%= notice %></p>

<h1>Instagrams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instagrams.each do |instagram| %>
      <tr>
        <td><%= link_to 'Show', instagram %></td>
        <td><%= link_to 'Edit', edit_instagram_path(instagram) %></td>
        <td><%= link_to 'Destroy', instagram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Instagram', new_instagram_path %>
